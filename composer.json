{
    "name": "kelcampus/laravel-repository",
    "description": "Cópia do pacote Artesãos Warehouse Tag 3.0.0 ajustado para o Laravel 10",
    "keywords": [
        "laravel",
        "laravel10",
        "repository",
        "eloquent",
        "model"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kelven Garcia Campos",
            "email": "kelcampus@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/pagination": "^10.0",
        "illuminate/contracts": "^10.0",
        "league/fractal": "~0.20"
    },
    "require-dev": {
        "illuminate/container": "^10.0",
        "illuminate/http": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Kelcampus\\LaravelRepositoy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kelcampus\\LaravelRepositoy\\LaravelRepositoyServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
